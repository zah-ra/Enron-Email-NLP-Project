        "###### \"body\" -> \"main_message\" -> \"final_main\" -> \"clean_main_message\"\n",
        "def clean_email_text(text):\n",
        "    if not isinstance(text, str):\n",
        "        return \"\"\n",
        "    text = text.replace('\\t', ' ').replace('\\r', ' ').replace(' > ', ' ').replace('\\n', ' ')\n",
        "    text = re.sub(r'\\s+', ' ', text)\n",
        "    text = text.replace('\\\\', ' ')\n",
        "    text = ''.join(c for c in text if c.isprintable()) # any leftover special (non-printable) characters\n",
        "    text = text.strip()\n",
        "    return text\n",
        "\n",
        "sample_emails[\"clean_main_message\"] = sample_emails[\"final_main\"].apply(clean_email_text)"
