      "source": [
        "import pandas as pd\n",
        "data_dir = \"/content/drive/MyDrive/Enron-emails.csv\"\n",
        "raw_emails = pd.read_csv(data_dir)"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": null,
      "metadata": {
        "id": "Lhr-hiGt2e09"
      },
      "outputs": [],
      "source": [
        "import email\n",
        "subjects = []\n",
        "senders = []\n",
        "recipients = []\n",
        "dates = []\n",
        "bodies = []\n",
        "\n",
        "for m in raw_emails['message']:\n",
        "    try:\n",
        "        msg = email.message_from_string(m)\n",
        "\n",
        "        subjects.append(msg.get('Subject', ''))\n",
        "        senders.append(msg.get('From', ''))\n",
        "        recipients.append(msg.get('To', ''))\n",
        "        dates.append(msg.get('Date', ''))\n",
        "\n",
        "        if msg.is_multipart():\n",
        "            for part in msg.walk():\n",
        "                if part.get_content_type() == 'text/plain':\n",
        "                    body = part.get_payload(decode=True)\n",
        "                    bodies.append(body.decode(errors='ignore') if body else '')\n",
        "                    break\n",
        "        else:\n",
        "            body = msg.get_payload(decode=True)\n",
        "            bodies.append(body.decode(errors='ignore') if body else '')\n",
        "\n",
        "    except:\n",
        "        subjects.append('')\n",
        "        senders.append('')\n",
        "        recipients.append('')\n",
        "        dates.append('')\n",
        "        bodies.append('')\n",
        "\n",
        "emails = pd.DataFrame({\n",
        "    'subject': subjects,\n",
        "    'from': senders,\n",
        "    'to': recipients,\n",
        "    'date': dates,\n",
        "    'body': bodies\n",
        "})"
      ]
    },
