        "import re\n",
        "import swifter\n",
        "\n",
        "pattern = re.compile(r'-{2,}.*?(forwarded by|original message).*', re.IGNORECASE)\n",
        "\n",
        "def extract_main_message(txt):\n",
        "    if not isinstance(txt, str):\n",
        "        return \"\"\n",
        "\n",
        "    matches = list(pattern.finditer(txt))\n",
        "    if not matches:\n",
        "        return txt.strip()\n",
        "\n",
        "    first_match_start = matches[0].start()\n",
        "    pre = txt[:first_match_start].strip()\n",
        "    if pre:\n",
        "        return pre\n",
        "\n",
        "    subject_splitter = re.compile(r'.*?Subject:.*?\\n\\n', flags=re.IGNORECASE | re.DOTALL)\n",
        "    if len(matches) == 1:\n",
        "        after_first = matches[0].end()\n",
        "        section = txt[after_first:]\n",
        "        subject_split = subject_splitter.split(section)\n",
        "        if len(subject_split) > 1:\n",
        "            return subject_split[1].strip()\n",
        "        else:\n",
        "            return section.strip()\n",
        "\n",
        "    if len(matches) > 1:\n",
        "        after_first = matches[0].end()\n",
        "        before_second = matches[1].start()\n",
        "        section = txt[after_first:before_second]\n",
        "        subject_split = subject_splitter.split(section)\n",
        "        if len(subject_split) > 1:\n",
        "            return subject_split[1].strip()\n",
        "        else:\n",
        "            return section.strip()\n",
        "\n",
        "    return txt[matches[0].end():].strip()\n",
        "\n",
        "\n",
        "new_samples[\"main_message\"] = new_samples[\"body\"].swifter.apply(extract_main_message)\n",
        "\n",
        "# emails[\"main_message\"] = emails[\"body\"].swifter.apply(extract_main_message)\n"
      ]
    },
