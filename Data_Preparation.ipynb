        "]\n",
        "\n",
        "def is_spam(text, sender):\n",
        "    text = text.lower()\n",
        "\n",
        "    if any(keyword in text for keyword in [\"content-transfer-encoding\", \"unsubscribe\", \"promotion\", \"click here\", \"special offer\", \"limited offer\"]):\n",
        "        return True\n",
        "    if text.count(\"[image]\") > 3 or text.count(\"<url>\") > 3:\n",
        "        return True\n",
        "    if sender:\n",
        "        if sender.strip().lower() in spam_senders:\n",
        "            return True\n",
        "\n",
        "    return False\n",
        "\n",
        "new_samples = new_samples[\n",
        "    ~new_samples.apply(lambda row: is_spam(row['main_message'], sender=row['from']), axis=1)\n",
        "]\n",
        "new_samples.reset_index(drop=True)"
      ]
