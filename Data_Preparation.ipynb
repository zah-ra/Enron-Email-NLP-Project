        "def clean_messy_body(text):\n",
        "    reply_splitter = re.compile(r'(^|\\n)\\s*(From:|To:|Cc:|Subject:|Date:|\\b\\d{1,2}/\\d{1,2}/\\d{2,4}\\s+\\d{1,2}:\\d{2}\\s*(AM|PM))', re.IGNORECASE)\n",
        "\n",
        "    if not isinstance(text, str):\n",
        "        return \"\"\n",
        "\n",
        "    match = reply_splitter.search(text)\n",
        "    if match:\n",
        "        clean_text = text[:match.start()].strip()\n",
        "    else:\n",
        "        clean_text = text.strip()\n",
        "\n",
        "    return clean_text\n",
        "\n",
        "\n",
        "new_samples[\"final_main\"] = new_samples[\"main_message\"].apply(clean_messy_body)"
      ]
